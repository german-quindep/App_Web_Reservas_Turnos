<div class="container py-4">
<h4 class="text-info">Lista De Reservas Ocupadas</h4>
<table id="listar-reservas" class="table table-striped table-bordered" style="width:100%">
        <thead>
            <tr>
                <th>Nombre del equipo</th>
                <th>Marca</th>
                <th>Desde</th>
                <th>Hasta</th>
            </tr>
        </thead>
        <tbody>
            {{#each reservas}}
            <tr>
                <td>{{Nombre_equipo}}</td>
                <td>{{Marca_equipo}}</td>
                <td>{{Reserva_Dia}}</td>
                <td>{{Reserva_Fin}}</td>
                </tr>
            {{else}}
            {{/each}}
        </tbody>
        <tfoot>
            <tr>
                <th>Nombre del Equipo</th>
                <th>Marca</th>
                <th>Desde</th>
                <th>Hasta</th>
            </tr>
        </tfoot>
    </table>
    </div>